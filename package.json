{
  "name": "postrust",
  "version": "1.1.0",
  "description": "Ultra-fast Dutch postcode lookup with embedded data",
  "main": "index.js",
  "types": "index.d.ts",
  "keywords": [
    "postcode",
    "dutch",
    "lookup",
    "address",
    "napi",
    "rust",
    "fast"
  ],
  "author": {
    "name": "Mees Dekker",
    "email": "me@meesdekker.xyz"
  },
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/LithiumOx/postrust-node.git"
  },
  "engines": {
    "node": ">= 10"
  },
  "scripts": {
    "build": "napi build --platform --release",
    "build:debug": "napi build --platform",
    "prepublishOnly": "napi prepublish -t npm",
    "artifacts": "napi artifacts"
  },
  "devDependencies": {
    "@napi-rs/cli": "^3.0.4"
  },
  "napi": {
    "binaryName": "postrust-npm",
    "targets": [
      "x86_64-apple-darwin",
      "aarch64-apple-darwin",
      "x86_64-unknown-linux-gnu",
      "aarch64-unknown-linux-gnu"
    ]
  },
  "files": [
    "index.js",
    "index.d.ts"
  ],
  "optionalDependencies": {
    "postrust-darwin-x64": "1.1.0",
    "postrust-darwin-arm64": "1.1.0",
    "postrust-linux-x64-gnu": "1.1.0",
    "postrust-linux-arm64-gnu": "1.1.0"
  }
}